APP_MODULES = audioboo-ogg audioboo-flac audioboo-native
APP_PROJECT_PATH = @PATH@
APP_OPTIM = @OPTIMIZATION@

AUDIOBOO_NATIVE_FLAGS = \
	-Isources/AudioBoo/config \
	-Isources/AudioBoo/ogg/include \
	-DVERSION=\"1.2\" \
	-Isources/AudioBoo/flac/include \
	-Isources/AudioBoo/flac/src/libFLAC/include

APP_CFLAGS += $(AUDIOBOO_NATIVE_FLAGS)
APP_CXXFLAGS += $(AUDIOBOO_NATIVE_FLAGS)
